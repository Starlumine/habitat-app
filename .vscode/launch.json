    "version": "0.2.0",
    "configurations": [
        {
            "name": "habitat_application",
            "request": "launch",
            "type": "dart",
            "program": "habitat_application/lib/main.dart",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "habitat_application (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "habitat_application/lib/main.dart",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "habitat_application (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "habitat_application/lib/main.dart",
            "cwd": "${workspaceFolder}"
        }
    ]
}
